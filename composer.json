{
  "name": "foxftw/wiki3d-extension",
  "type": "mediawiki-extension",
  "require-dev": {
	"jakub-onderka/php-parallel-lint": "0.9.2",
	"mediawiki/mediawiki-codesniffer": "0.7.2"
  },
  "scripts": {
	"fix": "phpcbf",
