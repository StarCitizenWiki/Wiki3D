      "dependencies": [
        "ext.w3d.threejs"
      ]
    },
    "ext.w3d.collada": {
      "scripts": [
        "modules/threejs/collada/ColladaLoader.js",
        "modules/ext.w3d.Collada.js"
      ],
      "dependencies": [
        "ext.w3d.threejs"
      ]
    },
    "ext.w3d.viewer": {
      "scripts": [
        "modules/threejs/controls/OrbitControls.js"
      ],
      "styles": [
        "modules/ext.w3d.shipviewer.css"
      ],
      "messages": [],
      "dependencies": []
